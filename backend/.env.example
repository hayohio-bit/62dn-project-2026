# Government API (공공데이터포털 구조동물조회) - application.yml animal.api.service-key
GOV_API_KEY=your_government_api_service_key
# GET 요청 시 특수문자(+, =) 인코딩 필요 시 사용 (선택)
GOV_API_KEY_ENCODED=

# Kakao (application.yml kakao.*)
KAKAO_REST_API_KEY=your_kakao_rest_api_key
KAKAO_JAVASCRIPT_KEY=your_kakao_javascript_key
KAKAO_ADMIN_KEY=your_kakao_admin_key

# API 제한 (application.yml api.config)
API_CALL_TIMEOUT_SECONDS=30
API_RETRY_COUNT=3
API_DAILY_LIMIT=10000

# DB / 기타
DB_USERNAME=root
DB_PASSWORD=your_password
RESEND_API_KEY=re_your_api_key
JWT_SECRET=your_jwt_secret_key_at_least_32_characters_long
PROD_DB_URL=jdbc:mysql://your_prod_db_host:3306/dn_platform
PROD_DB_USERNAME=prod_user
PROD_DB_PASSWORD=prod_password
