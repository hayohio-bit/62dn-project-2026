# 로컬에서 아래 내용을 .gitignore 파일로 생성
cat > .gitignore << 'EOF'
# Node (Frontend)
node_modules/
dist/
build/
.env.local
.env.*.local
npm-debug.log*
yarn-debug.log*
.DS_Store

# Java (Backend)
target/
*.class
*.jar
*.war
.idea/
*.iml
*.iws
*.ipr
.classpath
.project
.settings/
bin/
.vscode/

# Environment
.env
.env.dev
.env.prod
application-local.yml
application-dev.yml

# OS
.DS_Store
Thumbs.db

# IDE
.vscode/
.idea/
*.swp
*.swo
*~

# Logs
*.log
logs/
EOF